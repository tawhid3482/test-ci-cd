generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  name     String
  email    String @unique
  password String
  phone    String
  role     String
  gender   String
  status   String
}
enum Role {
  SUPER_ADMIN
  ADMIN
  USER
}
